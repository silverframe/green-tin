{
  "name": "green-tin",
  "version": "1.0.0",
  "description": "an itinerary sharing platform",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev-start": "nodemon server.js",
    "build": "browserify -t [ babelify ] ./build/js/main.js -o ./build/js/client.js -o",
    "dev-build": "watchify -t [ babelify ] ./build/js/main.js -o ./build/js/client.js -o",
    "dev": "npm run dev-build & npm run dev-start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/silverframe/green-tin.git"
  },
  "keywords": [
    "itinerary"
  ],
  "author": "Stefanie Seah",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/silverframe/green-tin/issues"
  },
  "homepage": "https://github.com/silverframe/green-tin#readme",
  "dependencies": {
    "connect-history-api-fallback": "^1.2.0",
    "react": "^15.0.2",
    "react-addons": "^0.9.1-deprecated",
    "react-dom": "^15.0.2",
    "underscore": "^1.8.3",
    "whatwg-fetch": "^0.11.0"
  },
  "devDependencies": {
    "babel": "5.8.23",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "6.4.0",
    "browser-sync": "^2.12.5",
    "browserify": "^13.0.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-notify": "^2.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-stylus": "^2.3.1",
    "gulp-uglify": "^1.5.3",
    "jquery": "^2.2.3",
    "react-router": "^2.4.0",
    "standard": "^6.0.8",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  }
}
